{
  "name": "lostark-dynamic-server",
  "version": "1.0.0",
  "description": "로스트아크 게임 데이터를 제공하는 API 서버",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "lostark",
    "api",
    "game",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios-retry": "^4.5.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "jsdom": "^26.1.0",
    "radis": "^2.0.0",
    "redis": "^5.5.6",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
